                document.getElementById(x).style.borderWidth = 1.5;
                document.getElementById(x).style.boxShadow = '0 0 10px rgba(227, 81, 82, 1)';
                return false;
            }
        }

        function validateNewInstallForm(){
            var error = 0;
            if(!validateSummary('summary')){
                document.getElementById('summary').style.borderColor ='#e35152';
                document.getElementById('summary').style.boxShadow = '0 0 10px rgba(227, 81, 82, 1)';
                document.getElementById('summary').focus();
                error++;
            }
            if(!validateDueDate('dueDate')){
                document.getElementById('dueDate').style.borderColor ='#e35152';
                document.getElementById('dueDate').style.boxShadow = '0 0 10px rgba(227, 81, 82, 1)';
                document.getElementById('dueDate').focus();
                error++;
            }
            if(!validateProjName('projName')){
                document.getElementById('projName').style.borderColor ='#e35152';
                document.getElementById('projName').style.boxShadow = '0 0 10px rgba(227, 81, 82, 1)';
                document.getElementById('projName').focus();
                error++;
            }
            if(!validateWorkQ('workQ')){
                document.getElementById('workQ').style.borderColor ='#e35152';
                document.getElementById('workQ').style.boxShadow = '0 0 10px rgba(227, 81, 82, 1)';
                document.getElementById('workQ').focus();
                error++;
            }
            if(!validateDetailDesc('detailDesc')){
                document.getElementById('detailDesc').style.borderColor ='#e35152';
                document.getElementById('detailDesc').style.boxShadow = '0 0 10px rgba(227, 81, 82, 1)';
                document.getElementById('detailDesc').focus();
                error++;
            }
            if(!validateHostName('hostName')){
                document.getElementById('hostName').style.borderColor ='#e35152';
                document.getElementById('hostName').style.boxShadow = '0 0 10px rgba(227, 81, 82, 1)';
                document.getElementById('hostName').focus();
                error++;
            }
            if(!validateWbsCode('wbsCode')){
                document.getElementById('wbsCode').style.borderColor ='#e35152';
                document.getElementById('wbsCode').style.boxShadow = '0 0 10px rgba(227, 81, 82, 1)';
                document.getElementById('wbsCode').focus();
                error++;
            }

            if(error > 0){
                return false;
            }
        }

        function validateTroubleTicketForm(){
            var error = 0;
            if(!validateSummary('summary')){
                document.getElementById('summary').style.borderColor ='#e35152';
                document.getElementById('summary').style.boxShadow = '0 0 10px rgba(227, 81, 82, 1)';
