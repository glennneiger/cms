            if(re.test(document.getElementById(x).value)){
                document.getElementById(x).style.borderColor ='#66cc00';
                document.getElementById(x).style.borderWidth = 1.5;
                document.getElementById(x).style.boxShadow = '0 0 10px rgba(102, 204, 0, 1)';
                return true;
            }else{
                document.getElementById(x).style.borderColor ='#e35152';
                document.getElementById(x).style.borderWidth = 1.5;
